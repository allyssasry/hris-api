{
  "version": 2,
  "installCommand": "npm install",
  "buildCommand": "npx prisma@6.17.1 generate && npx prisma@6.17.1 db push --accept-data-loss",
  "outputDirectory": ".",
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/server.js"
    }
  ]
}
